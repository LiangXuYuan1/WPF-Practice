<Window x:Class="MergeResourceDictionary.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525" >
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Dictionary4.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <sys:String x:Key="StringFromWindow">StringFromWindow</sys:String>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <StackPanel>
            <TextBlock Text="{DynamicResource StringFromWindow}"/>
            <TextBlock Text="{DynamicResource StringFromDictionary1}"/>
            <TextBlock Text="{DynamicResource StringFromDictionary2}"/>
            <TextBlock Text="{DynamicResource StringFromDictionary3}"/>
            <TextBlock Text="{DynamicResource StringFromDictionary4}"/>
        </StackPanel>
    </Grid>
</Window>
